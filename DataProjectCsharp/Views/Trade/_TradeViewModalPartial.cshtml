
<h4>Trade view for the portfolio</h4>
@model IEnumerable<DataProjectCsharp.Models.Trade>

<div class="modal fade" id="viewTrades">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class=" = modal-header">
                <h5 class="modal-title" id="viewTradeLabel">Trades for portfolio [Your Portfolio Name Here]</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body">
                <p></p>
                <p>My DataTable will go here</p>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-primary">Create a new Trade</button>
                <button type="button" class="btn btn-secondary" data-dismiss="modal">Cancel</button>
            </div>
        </div>
    </div>
</div>
@section Scripts{
    <partial name="_ValidationScriptsPartial" />
}